  "keywords": [
    "anilist",
    "manga",
    "anime",
    "suggest"
  ],
  "author": "Tibix & Sayykii",
  "license": "MIT",
  "dependencies": {
    "@acegoal07/discordjs-pagination": "^1.2.6",
    "@discordjs/rest": "^0.3.0",
    "axios": "^0.21.1",
    "canvas": "^2.8.0",
    "discord-api-types": "^0.27.2",
    "discord.js": "^14.0.3",
    "dotenv-flow": "^3.2.0",
    "humanize-duration": "^3.27.0",
    "ms": "^2.1.3",
    "node-rsa": "^1.1.1",
    "nodemon": "^2.0.12",
    "sequelize": "^6.17.0",
