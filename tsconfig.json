{
    "ts-node": {
        "require": [
            "tsconfig-paths/register"
        ],
    },
    "compilerOptions": {
        "moduleResolution": "node",
        "baseUrl": ".",
        "target": "ES6",
        "paths": {
            "#Utils/*": [
                "./src/Utils/*"
            ],
            "#Commands/*": [
                "./src/Commands/*"
            ],
            "#Structures/*": [
                "./src/Structures/*"
            ],
            "#GraphQL/*": [
                "./src/GraphQL/*"
            ],
            "#Models/*": [
                "./src/Database/Models/*"
            ],
            "#Caching/*": [
                "./src/Caching/*"
            ],
            "#RSA/*": [
                "./src/RSA/*"
            ],
            "#Middleware/*": [
                "./src/Middleware/*"
            ]
        }
    }
}